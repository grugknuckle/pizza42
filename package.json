{
  "name": "pizza-pizza",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "backend-dev": "nodemon startup",
    "dev": "npm-run-all --parallel serve backend-dev",
    "postinstall": "npm run build",
    "start": "node startup"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.13.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/vue-fontawesome": "^0.1.10",
    "axios": "^0.21.1",
    "core-js": "2.6.9",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "helmet": "^3.23.3",
    "highlight.js": "^10.4.1",
    "jwks-rsa": "^1.11.0",
    "morgan": "^1.10.0",
    "serve-static": "^1.14.1",
    "vue": "^2.6.12",
    "vue-router": "^3.4.9",
    "vuetify": "^2.4.0",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^4.5.9",
    "@vue/eslint-config-prettier": "^3.0.5",
    "babel-eslint": "^10.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.14.1",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "vue-cli-plugin-vuetify": "~2.4.1",
    "vue-template-compiler": "^2.6.12",
    "vuetify-loader": "^1.7.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true
    },
    "plugins": [
      "vue"
    ],
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": [
        "error",
        {
          "allow": [
            "error",
            "warn"
          ]
        }
      ],
      "quotes": [
        "warn",
        "single"
      ],
      "semi": [
        2,
        "never"
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/grugknuckle/pizza42"
  }
}
